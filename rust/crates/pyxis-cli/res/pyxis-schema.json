{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Settings",
  "type": "object",
  "properties": {
    "abacus": {
      "$ref": "#/$defs/Settings"
    },
    "trail": {
      "$ref": "#/$defs/Settings2"
    }
  },
  "required": [
    "abacus",
    "trail"
  ],
  "$defs": {
    "OutputFormat": {
      "type": "string",
      "enum": [
        "Simple",
        "Verbose",
        "Json"
      ]
    },
    "Settings": {
      "type": "object",
      "properties": {
        "output_format": {
          "$ref": "#/$defs/OutputFormat"
        }
      },
      "required": [
        "output_format"
      ]
    },
    "Settings2": {
      "type": "object",
      "properties": {
        "baud_rate": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0
        },
        "capacity": {
          "type": "integer",
          "format": "uint",
          "minimum": 0
        },
        "port": {
          "type": "string"
        },
        "tab_coord": {
          "$ref": "#/$defs/TabCoordSettings"
        }
      },
      "required": [
        "port",
        "baud_rate",
        "capacity",
        "tab_coord"
      ]
    },
    "TabCoordSettings": {
      "type": "object",
      "properties": {
        "custom_cs": {
          "type": "string"
        }
      },
      "required": [
        "custom_cs"
      ]
    }
  }
}